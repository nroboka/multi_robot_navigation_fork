    </joint>

    <link name='spoiler'>  
      <inertial>
          <mass value="15.0"/>
          <origin xyz="0.0 0 0" rpy=" 0 0 0"/>
          <inertia
              ixx="0.000833" ixy="0" ixz="0"
              iyy="0.0075" iyz="0"
              izz="0.008333"
